<urlLists>
    <require>
        <define>parseURLReq</define>
        <path>process.mainModule.exports["corePath"] + '/src/serverroot/common/parseURLRequire'</path>
    </require>
    <require>
        <define>sasconfigapi</define>
        <path>./serviceappliancesetconfig.api</path>
    </require>
    <item>
        <url>/api/tenants/config/service-appliance-sets</url>
        <method>post</method>
        <feature>sasconfig</feature>
        <callback>sasconfigapi.createServiceApplianceSet</callback>
    </item>
    <item>
        <url>/api/tenants/config/service-appliance-set/:id</url>
        <method>delete</method>
        <feature>sasconfig</feature>
        <callback>sasconfigapi.deleteServiceApplianceSet</callback>
    </item>

</urlLists>

