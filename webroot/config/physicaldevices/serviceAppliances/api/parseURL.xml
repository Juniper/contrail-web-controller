<urlLists>
    <require>
        <define>parseURLReq</define>
        <path>process.mainModule.exports["corePath"] + '/src/serverroot/common/parseURLRequire'</path>
    </require>
    <require>
        <define>saconfigapi</define>
        <path>./serviceapplianceconfig.api</path>
    </require>
    <item>
        <url>/api/tenants/config/service-appliances</url>
        <method>post</method>
        <feature>saconfig</feature>
        <callback>saconfigapi.createServiceAppliance</callback>
    </item>

    <item>
        <url>/api/tenants/config/service-appliance/:id</url>
        <method>delete</method>
        <feature>saconfig</feature>
        <callback>saconfigapi.deleteServiceAppliance</callback>
    </item>

    <item>
        <url>/api/tenants/config/service-appliance/:id</url>
        <method>put</method>
        <feature>saconfig</feature>
        <callback>saconfigapi.updateServiceAppliance</callback>
    </item>

    <item>
        <url>/api/tenants/config/service-appliance/physical-interface/:id</url>
        <method>put</method>
        <feature>saconfig</feature>
        <callback>saconfigapi.updatePhysicalInterface</callback>
    </item>

</urlLists>

