    <div>
        <div class="row-fluid">
            <div class="row-fluid">
                <div class="span2"><label>Name</label></div>
                <div class="span10"> {{name}}</div>
            </div>
            <div class="row-fluid">
                <div class="span2"><label>UUID</label></div>
                <div class="span10"> {{uuid}}</div>
            </div>
	    <div class="row-fluid">
                <div class="span2"><label>Physical Interfaces</label></div>
                <div class="span10">{{interfaces}}</div>
            </div>
        </div>
