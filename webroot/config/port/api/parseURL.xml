<urlLists>
    <require>
        <define>parseURLReq</define>
        <path>process.mainModule.exports["corePath"] + '/src/serverroot/common/parseURLRequire'</path>
    </require>
    <require>
        <define>routesHandler</define>
        <path>process.mainModule.exports["corePath"] + '/src/serverroot/web/routes/handler'</path>
    </require>
    <require>
        <define>portConfig</define>
        <path>./portConfig.api</path>
    </require>
    <item>
        <url>/api/tenants/config/create-port</url>
        <method>post</method>
        <feature>portconfig</feature>
        <callback>portConfig.createPort</callback>
    </item>
    <item>
        <url>/api/tenants/config/delete-port/:portid</url>
        <method>delete</method>
        <feature>portconfig</feature>
        <callback>portConfig.deletePort</callback>
    </item>
    <item>
        <url>/api/tenants/config/interface-attach</url>
        <method>post</method>
        <feature>portconfig</feature>
        <callback>portConfig.portAttach</callback>
     </item>
     <item>
        <url>/api/tenants/config/interface-detach</url>
        <method>delete</method>
        <feature>portconfig</feature>
        <callback>portConfig.portDetach</callback>
    </item>
</urlLists>
