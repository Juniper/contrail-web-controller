<urlLists>
    <!-- Define module name here which is being referenced in the generated 
         file
     -->
    <require>
        <define>parseURLReq</define>
        <path>process.mainModule.exports["corePath"] + '/src/serverroot/common/parseURLRequire'</path>
    </require>
